<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/id_order_history"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"

        android:text="0023420354"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7" />

    <TextView
        android:id="@+id/txtPriceHistory"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:gravity="center"
        android:text="56e"
        android:textSize="29dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/id_order_history"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtDateHistory"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"

        android:text="85/5/2010"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toBottomOf="@+id/id_order_history" />

    <TextView
        android:id="@+id/txtTimeHistory"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:gravity="center_vertical"

        android:text="4:45:34"
        app:layout_constraintEnd_toStartOf="@+id/guideline9"
        app:layout_constraintStart_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toBottomOf="@+id/txtDateHistory" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#41000000"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTimeHistory" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.27" />

    <TextView
        android:paddingStart="20dp"
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="Id pedido:"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:paddingStart="20dp"
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="Fecha:"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:paddingStart="20dp"
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="30dp"
        android:text="Hora:"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />


</androidx.constraintlayout.widget.ConstraintLayout>